<h1>プロフィール編集</h1>
  <%= form_with model: @user, method: :patch do |f| %>
    <label for="inputName">ユーザーネーム</label></br>
    <%= f.text_field :name, placeholder: "ユーザーネーム" %></br>
    <label for="inputImage">プロフィール画像</label></br>
    <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %></br>
    <%= f.submit "変更を保存"%>
  <% end %>